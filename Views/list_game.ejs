<head>
    <title>SplashDash</title>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="">
    <meta name="MobileOptimized" content="320">
    <!--Start Style -->
    <link rel="stylesheet" type="text/css" href="/assets/css/fonts.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/icofont.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/style.css">
    <link rel="stylesheet" type="text/css" href="/assets/css/custom.css">
	<link rel="stylesheet" id="theme-change" type="text/css" href="#">
    <!-- Favicon Link -->
    <link rel="shortcut icon" type="image/png" href="assets/images/favicon.png">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
</head>


<body>
	<div class="loader">
	  <div class="spinner">
		<img src="/assets/images/loader.gif" alt=""/>
	  </div> 
	</div>
    <!-- Main Body -->
    <div class="page-wrapper">
        <!-- Header Start -->
        <%- include('./header'); -%>
        <!-- Sidebar Start -->
        <%- include('./sidebar'); -%>
        <!-- Container Start -->
        <div class="page-wrapper">
            <div class="main-content">
                <!-- Page Title Start -->
                <div class="row">
                    
                </div>
                <!-- Table Start -->
                <table id="table1" class="table table-styled mb-0">
                    <thead>
                    
                        <tr>
                            <th>#</th>
                            <th>Name</th>
                            <th>Open Time</th>
                            <th>Close Time</th>
                            <th>Status</th>
                           
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach((element,index) => { %>
                                                
                            <tr>
                                <td><%= index+1 %></td>                                                   
                                <td><%= element.game_name %></td>
                                <td><%= element.open_time %></td>
                                <td><%= element.close_time %></td> 
                                <td><%= element.status %></td>
                                <td > 
                                    <div style="display: flex;flex-direction: row; justify-content: space-around;">
                                        <form><a href="/add-game/edit/<%= element._id %>"> Edit</a> </form>
                                        <form action="post"><a href="/add-game/delete/<%= element._id %>">Delete </a></form>
                                    </div>
                                    
                                     
                                    </td>                                                       

                            </tr>
                            
                            <% }) %>
							
                        
                    </tbody>
                </table>
   
			</div> 
            
        </div>   
		</div>    
	</div>    
   <!-- Preview Setting Box -->
	
    <!-- Preview Setting -->
	
    <!-- Script Start -->
    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/popper.min.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/custom.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
<script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
<link
rel="stylesheet"
type="text/css"
href="http://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.4/css/jquery.dataTables.css"
/>
<script>
    $(document).ready(function () {
        $('#table1').DataTable({
            "lengthMenu": [[10, 25, 50, -1], [10, 25, 50, "All"]],
            "paginate": {
                     "first": "First page"
                     }
        });
    });
</script>
</body>

</html>